{
  "name": "@settlemint/solidity-legacy-templateset",
  "version": "3.63.2",
  "private": true,
  "description": "A launchpad demo contract",
  "scripts": {
    "lint": "yarn solhint --fix 'contracts/**/*.sol'",
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test",
    "smartcontract:deploy": "yarn hardhat deploy --network ${DEFAULT_NODE}",
    "smartcontract:deploy:reset": "yarn hardhat deploy --network ${DEFAULT_NODE} --reset",
    "middleware:package": "tar vzcf deployments-for-middleware.tar.gz ./deployments/${DEFAULT_NODE} && echo 'Download deployments-for-middleware.tar.gz from the IDE and import the JSON files inside into the Legacy Middleware'"
  },
